<Window x:Class="MapEditor.WAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapEditor"
        mc:Ignorable="d"
        Title="增加字段" Height="120" Width="270" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="button" Content="添加" HorizontalAlignment="Left" Margin="165,34,0,0" VerticalAlignment="Top" Width="75" Click="AddKey_Click"/>
        <TextBox x:Name="NewKey" HorizontalAlignment="Left" Height="19" Margin="18,21,0,0" TextWrapping="Wrap" Text="新字段" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="NewValue" HorizontalAlignment="Left" Height="19" Margin="18,51,0,0" TextWrapping="Wrap" Text="默认值" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
