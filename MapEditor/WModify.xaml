<Window x:Class="MapEditor.WModify"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapEditor"
        mc:Ignorable="d"
        Title="WModify" Height="120" Width="420" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top" Width="120" Height="19" SelectionChanged="Modify_SelectionChanged"/>
        <TextBox x:Name="modifyValue" HorizontalAlignment="Left" Height="19" Margin="152,39,0,0" TextWrapping="Wrap" Text="值" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="修改" HorizontalAlignment="Left" Margin="310,39,0,0" VerticalAlignment="Top" Width="76" Click="Modify_Click"/>

    </Grid>
</Window>
