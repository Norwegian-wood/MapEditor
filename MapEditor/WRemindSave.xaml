<Window x:Class="MapEditor.WRemindSave"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapEditor"
        mc:Ignorable="d"
        Title="提示" Height="120" Width="270" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="42,21,0,0" TextWrapping="Wrap" Text="地图信息发生更改，是否保存？" VerticalAlignment="Top" Width="178"/>
        <Button x:Name="button" Content="保存" HorizontalAlignment="Left" Margin="42,54,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="取消" HorizontalAlignment="Left" Margin="145,54,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>

    </Grid>
</Window>
